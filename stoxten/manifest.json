{
    "name": "StockWatch",
    "description": "Prototype project for company stock's tracking and note takeing. ",
    "version": "6.9",
    "manifest_version": 3,
    "icons": {
        "16": "img/16px.png",
        "48": "img/48px.png",
        "128": "img/128px.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["js/script.js"]
        }
    ]
}